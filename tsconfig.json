{
    "include": [
        "src/**/*"
    ],
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "sourceMap": true,
        "declaration": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "jsx": "react"
    },
    "exclude": [
        "./src/sample.exemple.tsx"
    ]
}